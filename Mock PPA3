def mergeSortedList(head1, head2):
# Create a dummy node to simplify handling the result list
dummy = Node(0)
tail = dummy

# Merge process
while head1 and head2:
    if head1.data < head2.data:
        tail.next = head1
        head1 = head1.next
    else:
        tail.next = head2
        head2 = head2.next
    tail = tail.next

# If either list has remaining elements, append them
if head1:
    tail.next = head1
elif head2:
    tail.next = head2

# Return head of merged list
return dummy.next
